{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "eslint-plugin-import-helpers",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": "error",
      "camelcase": "off",
      "import/no-unresolved": "error",
      "import/prefer-default-export": "off",
      "no-shadow": "off",
      "no-console": "off",
      "no-empty-function": "off",
      "import-helpers/order-imports": [
        "warn",
        {
          "newlinesBetween": "always",
          "groups": [
            "module",
            "/^@shared/",
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "alphabetize": {
            "order": "asc",
            "ignoreCase": true
          }
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.spec.js"
          ]
        }
      ]
    }
  }
  